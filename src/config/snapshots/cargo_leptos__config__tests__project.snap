            front_package: "example ..",
            front_profile: "debug",
            server_package: "example ..",
            server_target: "example (bin) ..",
            server_profile: "debug",
            watch: true,
            site: Site {
                file_reg: {},
                ext_file_reg: {},
            },
            paths: ProjectPaths {
                lib_crate_name: "example",
                site_root: "target/site",
                site_pkg_dir: "target/site/pkg",
