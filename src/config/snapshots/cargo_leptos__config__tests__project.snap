                site_addr: 127.0.0.1:3000,
                site_root: "target/site",
                site_pkg_dir: "pkg",
                style_file: Some(
                    "style/main.scss",
                ),
                assets_dir: Some(
                    "assets",
                ),
                reload_port: 3001,
